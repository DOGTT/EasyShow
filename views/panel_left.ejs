    <div id = "title">
        <div id="title1" class = "title_text"><%= index_info.pannel.title%></div> 
        <div id="title2" class = "title_text"><%= index_info.pannel.message%></div>
    </div>
    <img  id = "head_portrait" src=<%= index_info.pannel.head_portrait%> />
    <ul id = "myInfo">
        <li id = "about_me" class = "myInfo_li">
            <svg class="icon"><use xlink:href="#icon-hipster"></use></svg>
             <span class="m_span"><div><%= index_info.pannel.about_me%></div></span>
        </li>
        <li id = "sayhi" class = "myInfo_li">
            <svg class="icon "><use xlink:href="#icon-heart"></use></svg>
            <span class="m_span"><div><%= index_info.pannel.sayhi%></div></span>
        </li>
        <li id = "e_mail" class = "myInfo_li">
            <svg class="icon "><use xlink:href="#icon-mail"></use></svg>
            <a class="pannel_a" href="mailto:'<%= index_info.pannel.e_mail%>'"+><%= index_info.pannel.e_mail%></a>
        </li>
        <li id = "github" class = "myInfo_li">
            <svg class="icon icon-github"><use xlink:href="#icon-github"></use></svg>
            <a class="pannel_a" target="_blank" href=<%= index_info.pannel.github%>>Github</a>
        </li>
        
    </ul>
    <script>
        var panel = document.getElementById( 'panel' );
        var head_portrait = document.getElementById( 'head_portrait' );
        function resizeImg() {   
            var fullWidth = panel.clientWidth;
            var width =  (fullWidth*0.8);  
            head_portrait.style.width = width+ 'px';
            head_portrait.style.height = width+ 'px';
            console.log(fullWidth,width);
        }
        window.addEventListener( 'resize', resizeImg, false );
        resizeImg();
    </script>
</div>